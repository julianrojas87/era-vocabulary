era: a owl:Ontology ;
    dct:title "ERA vocabulary"@en ;
    rdfs:label "ERA vocabulary"@en ;
    dct:issued "2020-07-29"^^xsd:date ;
    dct:modified "2021-01-29"^^xsd:date ;
    cc:license <https://creativecommons.org/licenses/by/4.0/> ;
    dct:author <https://julianrojas.org/#me> ;
    dct:publisher "European Union Agency for Railways" ;
    dct:contributor "Ivaylo Velitchkov" ;
    dct:contributor "Polymnia Vasilopoulou" ;
    dct:contributor "Marina Aguado" ;
    dct:contributor "Dylan Van Assche" ;
    dct:contributor "Pieter Colpaert" ;
    rdfs:comment "Vocabulary defined by the European Union Agency for Railways to describe the concepts and relationships related to the European railway infrastructure and the vehicles authorized to operate over it."@en ;
    owl:versionInfo "v1.2.0" ;
    owl:imports schema:, era-skos:, eu-countries:, wgs:, geosparql: .


#################################################################
#
#    Classes
#
#################################################################

######## Abstraction Layer #########

era:MicroNode a owl:Class ;
    rdfs:label "Micro Node"@en ;
    rdfs:comment "An abstraction of an operational point, used to provide a simple topological description of the railway infrastructure."@en ;
    rdfs:isDefinedBy era: ;
    dct:created "2020-07-29"^^xsd:date ;
    dct:modified "2020-07-29"^^xsd:date ;
    vs:term_status "stable" .

era:MicroLink a owl:Class ;
    rdfs:label "Micro Link"@en ;
    rdfs:comment "An abstraction of a track that represents a physical rail connection between two different micro nodes."@en ;
    rdfs:isDefinedBy era: ;
    dct:created "2020-07-29"^^xsd:date ;
    dct:modified "2020-07-29"^^xsd:date ;
    vs:term_status "stable" .

era:NodePort a owl:Class ;
    rdfs:subClassOf geosparql:Geometry, wgs:Point;
    rdfs:label "Node Port"@en ;
    rdfs:comment "A port to enter or exit a micro node. Represents the connection between a micro link and a micro node."@en ;
    rdfs:isDefinedBy era: ;
    dct:created "2020-07-29"^^xsd:date ;
    dct:modified "2020-07-29"^^xsd:date ;
    vs:term_status "stable" .

era:InternalNodeLink a owl:Class ;
    rdfs:label "Internal Node Link"@en ;
    rdfs:comment "A specific track abstraction used to represent a micro link switch that is possible inside a micro node."@en ;
    rdfs:isDefinedBy era: ;
    dct:created "2020-07-29"^^xsd:date ;
    dct:modified "2020-07-29"^^xsd:date ;
    vs:term_status "stable" .

era:LineReference a owl:Class ;
    rdfs:label "Line Reference"@en ;
    rdfs:comment "A reference to a specific national railway line."@en ;
    rdfs:isDefinedBy era: ;
    dct:created "2021-01-29"^^xsd:date ;
    dct:modified "2021-01-29"^^xsd:date ;
    vs:term_status "stable" .


######## Implementation Layer #########

era:LineNationalId a owl:Class ;
    rdfs:label "Line National Id"@en ;
    rdfs:comment "Unique line identification within a member state."@en ;
    rdfs:isDefinedBy era: ;
    dct:created "2021-01-25"^^xsd:date ;
    dct:modified "2021-01-25"^^xsd:date ;
    vs:term_status "stable" .

era:OperationalPoint a owl:Class ;
    rdfs:label "Operational Point"@en ;
    rdfs:comment "An operational point (OP) means any location for train service operations, where train services may begin and end or change route, and where passenger or freight services may be provided; operational point also means any location at boundaries between Member States or infrastructure managers."@en ;
    rdfs:isDefinedBy era: ;
    rdfs:subClassOf geosparql:Feature, wgs:SpatialThing ; #geonames:Feature;
    dct:created "2020-07-29"^^xsd:date ;
    dct:modified "2020-09-01"^^xsd:date ;
    vs:term_status "stable" .

era:Track a owl:Class ;
    rdfs:label "Track"@en ;
    rdfs:comment "A running track that is used for train service movements."@en ;
    rdfs:isDefinedBy era: ;
    dct:created "2020-07-29"^^xsd:date ;
    dct:modified "2020-07-29"^^xsd:date ;
    vs:term_status "stable" .

era:Tunnel a owl:Class ;
    rdfs:label "Tunnel"@en ;
    rdfs:comment "A railway tunnel is an excavation or a construction around the track provided to allow the railway to pass for example higher land, buildings or water."@en ;
    rdfs:isDefinedBy era: ;
    dct:created "2020-07-29"^^xsd:date ;
    dct:modified "2020-07-29"^^xsd:date ;
    vs:term_status "stable" .

era:VehicleType a owl:Class ;
    rdfs:label "Vehicle Type"@en ;
    rdfs:comment "A vehicle type that has been authorized to operate on the EU railway infrastructure."@en ;
    rdfs:isDefinedBy era: ;
    dct:created "2020-07-29"^^xsd:date ;
    dct:modified "2020-07-29"^^xsd:date ;
    vs:term_status "stable" .

era:Vehicle a owl:Class ;
    rdfs:label "Vehicle"@en ;
    rdfs:comment "A specific vehicle or wagon able to operate over railway lines."@en ;
    rdfs:isDefinedBy era: ;
    dct:created "2020-11-22"^^xsd:date ;
    dct:modified "2020-11-22"^^xsd:date ;
    vs:term_status "stable" .

######## Others #########

era:Manufacturer a owl:Class ;
    rdfs:subClassOf schema:Organization ;
    rdfs:label "Manufacturer"@en ;
    rdfs:comment "A company or organization that manufactures vehicles."@en ;
    rdfs:isDefinedBy era: ;
    dct:created "2020-11-19"^^xsd:date ;
    dct:modified "2020-11-19"^^xsd:date ;
    vs:term_status "stable" .

era:VehicleKeeper a owl:Class ;
    rdfs:subClassOf schema:Organization ;
    rdfs:label "Vehicle Keeper"@en ;
    rdfs:comment "A company or organization that owns or operates a certain vehicle."@en ;
    rdfs:isDefinedBy era: ;
    dct:created "2020-11-23"^^xsd:date ;
    dct:modified "2020-11-23"^^xsd:date ;
    vs:term_status "stable" .

#################################################################
#
#    Object Properties
#
#################################################################

era:hasImplementation a owl:ObjectProperty ;
    owl:inverseOf era:hasAbstraction ;
    rdfs:label "has implementation"@en ;
    rdfs:comment "Property that links a topological abstraction entity with its physical representation."@en ;
    rdfs:isDefinedBy era: ;
    rdfs:domain [ a owl:Class ;
                  owl:unionOf ( era:MicroNode era:MicroLink )
                ] ;
    rdfs:range  [ a owl:Class ;
                  owl:unionOf ( era:OperationalPoint era:Track )
                ] ;
    dct:created "2020-07-29"^^xsd:date ;
    dct:modified "2020-07-29"^^xsd:date ;
    vs:term_status "stable" .

era:hasAbstraction a owl:ObjectProperty ;
    owl:inverseOf era:hasImplementation ;
    rdfs:label "has abstraction"@en ;
    rdfs:comment "Property that links a physical entity with its topological abstraction representation."@en ;
    rdfs:isDefinedBy era: ;
    rdfs:domain [ a owl:Class ;
                  owl:unionOf ( era:OperationalPoint era:Track )
                ] ;
    rdfs:range  [ a owl:Class ;
                  owl:unionOf ( era:MicroNode era:MicroLink )
                ] ;
    dct:created "2020-07-29"^^xsd:date ;
    dct:modified "2020-07-29"^^xsd:date ;
    vs:term_status "stable" .

era:opType a owl:ObjectProperty ;
    rdfs:label "operational point type"@en ;
    rdfs:comment "Indicates the type on an operational point."@en ;
    rdfs:isDefinedBy era: ;
    rdfs:domain era:OperationalPoint ;
    rdfs:range era-op-types:OperationalPointType ;
    dct:created "2020-07-29"^^xsd:date ;
    dct:modified "2020-07-29"^^xsd:date ;
    vs:term_status "stable" .

era:lineNationalId a owl:ObjectProperty ;
    rdfs:label "line national ID"@en ;
    rdfs:comment "Related national line identification."@en ;
    rdfs:isDefinedBy era: ;
    rdfs:range era:LineNationalId ;
    dct:created "2020-09-30"^^xsd:date ;
    dct:modified "2021-01-25"^^xsd:date ;
    vs:term_status "stable" .

era:lineReference a owl:ObjectProperty ;
    rdfs:label "line referece"@en ;
    rdfs:comment "Indicates a relationship with a specific national railway line."@en ;
    rdfs:isDefinedBy era: ;
    rdfs:range era:LineReference ;
    dct:created "2021-01-29"^^xsd:date ;
    dct:modified "2021-01-29"^^xsd:date ;
    vs:term_status "stable" .

era:startPort a owl:ObjectProperty ;
    rdfs:label "start port"@en ;
    rdfs:comment "Reference to the starting operational point's node port of a micro link or an internal node link."@en ;
    rdfs:isDefinedBy era: ;
    rdfs:domain [ a owl:Class ;
                  owl:unionOf ( era:MicroLink era:InternalNodeLink )
                ] ;
    rdfs:range era:NodePort ;
    dct:created "2020-07-29"^^xsd:date ;
    dct:modified "2020-07-29"^^xsd:date ;
    vs:term_status "stable" .

era:endPort a owl:ObjectProperty ;
    rdfs:label "end port"@en ;
    rdfs:comment "Reference to the ending operational point's node port of a micro link or an internal node link."@en ;
    rdfs:isDefinedBy era: ;
    rdfs:domain [ a owl:Class ;
                  owl:unionOf ( era:MicroLink era:InternalNodeLink )
                ] ;
    rdfs:range era:NodePort ;
    dct:created "2020-07-29"^^xsd:date ;
    dct:modified "2020-07-29"^^xsd:date ;
    vs:term_status "stable" .

era:belongsToNode a owl:ObjectProperty ;
    rdfs:label "belongs to node"@en ;
    rdfs:comment "Indicates the micro node that a node port belongs to."@en ;
    rdfs:isDefinedBy era: ;
    rdfs:domain era:NodePort ;
    rdfs:range era:MicroNode ;
    dct:created "2020-07-29"^^xsd:date ;
    dct:modified "2020-09-01"^^xsd:date ;
    vs:term_status "stable" .

era:startLocation a owl:ObjectProperty ;
    rdfs:subPropertyOf geosparql:hasGeometry, wgs:location ;
    rdfs:label "start location"@en ;
    rdfs:comment "References the geolocation of the start of a tunnel."@en ;
    rdfs:isDefinedBy era: ;
    rdfs:domain era:Tunnel ;
    rdfs:range [ a owl:Class ;
                  owl:unionOf ( geosparql:Geometry wgs:Point )
               ] ;
    dct:created "2020-07-29"^^xsd:date ;
    dct:modified "2020-07-29"^^xsd:date ;
    vs:term_status "stable" .

era:endLocation a owl:ObjectProperty ;
    rdfs:subPropertyOf geosparql:hasGeometry, wgs:location ;
    rdfs:label "end location"@en ;
    rdfs:comment "References the geolocation of the end of a tunnel."@en ;
    rdfs:isDefinedBy era: ;
    rdfs:domain era:Tunnel ;
    rdfs:range [ a owl:Class ;
                  owl:unionOf ( geosparql:Geometry wgs:Point )
               ] ;
    dct:created "2020-07-29"^^xsd:date ;
    dct:modified "2020-07-29"^^xsd:date ;
    vs:term_status "stable" .

era:hasTrack a owl:ObjectProperty ;
    rdfs:label "has track"@en ;
    rdfs:comment "References a rail track that goes through a tunnel."@en ;
    rdfs:isDefinedBy era: ;
    rdfs:domain era:Tunnel ;
    rdfs:range era:Track ;
    dct:created "2020-07-29"^^xsd:date ;
    dct:modified "2020-07-29"^^xsd:date ;
    vs:term_status "stable" .

era:passesThroughTunnel a owl:ObjectProperty ;
    rdfs:label "passes through tunnel"@en ;
    rdfs:comment "References a tunnel that a track goes through."@en ;
    rdfs:isDefinedBy era: ;
    rdfs:domain era:Track ;
    rdfs:range era:Tunnel ;
    dct:created "2020-07-29"^^xsd:date ;
    dct:modified "2020-07-29"^^xsd:date ;
    vs:term_status "stable" .

era:category a owl:ObjectProperty ;
    rdfs:label "vehicle category"@en ;
    rdfs:comment "Vehicle category."@en ;
    rdfs:isDefinedBy era: ;
    rdfs:domain era:VehicleType ;
    rdfs:range era-vehicle-types:Category ;
    dct:created "2020-07-29"^^xsd:date ;
    dct:modified "2020-11-19"^^xsd:date ;
    vs:term_status "stable" .

era:subCategory a owl:ObjectProperty ;
    rdfs:label "vehicle subcategory"@en ;
    rdfs:comment "Vehicle subcategory."@en ;
    rdfs:isDefinedBy era: ;
    rdfs:domain era:VehicleType ;
    rdfs:range era-vehicle-types:SubCategory ;
    dct:created "2020-07-29"^^xsd:date ;
    dct:modified "2020-11-19"^^xsd:date ;
    vs:term_status "stable" .

era:manufacturer a owl:ObjectProperty ;
    rdfs:label "manufacturer"@en ;
    rdfs:comment "Vehicle manufacturer company."@en ;
    rdfs:isDefinedBy era: ;
    rdfs:domain era:VehicleType ;
    rdfs:range era:Manufacturer ;
    dct:created "2020-07-29"^^xsd:date ;
    dct:modified "2020-11-19"^^xsd:date ;
    vs:term_status "stable" .

era:inCountry a owl:ObjectProperty ;
    rdfs:label "in country"@en ;
    rdfs:comment "Indicates the country in which an entity resides."@en ;
    rdfs:isDefinedBy era: ;
    rdfs:range skos:Concept ;
    dct:created "2020-09-01"^^xsd:date ;
    dct:modified "2020-11-20"^^xsd:date ;
    vs:term_status "stable" .

era:manufacturingCountry a owl:ObjectProperty ;
    rdfs:label "manufacturing country"@en ;
    rdfs:comment "Indicates the country in which a vehicle or vehicle type is manufactured."@en ;
    rdfs:isDefinedBy era: ;
    rdfs:domain [ a owl:Class ;
                  owl:unionOf ( era:Vehicle era:VehicleType )
                ] ;
    rdfs:range skos:Concept ;
    dct:created "2020-09-30"^^xsd:date ;
    dct:modified "2020-11-22"^^xsd:date ;
    vs:term_status "stable" .

era:authorizedCountry a owl:ObjectProperty ;
    rdfs:label "authorized country"@en ;
