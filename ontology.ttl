    dct:modified "2020-09-30"^^xsd:date ;
    vs:term_status "stable" .

era:vehicleType a owl:ObjectProperty ;
    rdfs:label "vehicle type"@en ;
    rdfs:comment "Indicates the vehicle type of a specific vehicle or wagon."@en ;
    rdfs:isDefinedBy era: ;
    rdfs:domain era:Vehicle ;
    rdfs:range era:VehicleType ;
    dct:created "2020-11-22"^^xsd:date ;
    dct:modified "2020-11-22"^^xsd:date ;
    vs:term_status "stable" .

era:operationalRestriction a owl:ObjectProperty ;
    rdfs:label "operational restriction"@en ;
    rdfs:comment "Indicates an operational restriction of vehicle or wagon."@en ;
    rdfs:isDefinedBy era: ;
